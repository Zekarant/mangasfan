{% extends 'base.html.twig' %}

{% block title %}Test Mangas'Fan V7 Symfony!{% endblock %}

{% block body %}
    <h1>{{  pluralize(pins|length, 'Pin') }}</h1>
    {%  for pin in pins %}
        <article>
            <h2>{{ pin.title }}</h2>
            <p>{{ pin.description }}</p>
        </article>
    {% endfor %}
{% endblock %}
